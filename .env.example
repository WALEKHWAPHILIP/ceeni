# Environment type (dev, prod, test)
DJANGO_ENV=dev

# Secret key placeholder, replace with your real key in .env
SECRET_KEY=your-secret-key-here

# Database name placeholder
DB_NAME=your_db_name

# Database username placeholder
DB_USER=your_db_user

# Database password placeholder (do NOT commit real passwords)
DB_PASSWORD=your_db_password

# Database host, default is localhost
DB_HOST=localhost

# Database port, default is 5432 for PostgreSQL
DB_PORT=5432
